font_family {{ .font }}
bold_font          auto
italic_font        auto
bold_italic_font   auto
font_size 13.0

window_padding_width 5
confirm_os_window_close 0

enable_audio_bell no

# BEGIN_KITTY_THEME
# Kanagawa
include current-theme.conf
# END_KITTY_THEME

map ctrl+shift+n new_os_window_with_cwd
